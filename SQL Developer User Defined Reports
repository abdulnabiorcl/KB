<display id="bb949ff9-0193-1000-8004-ac1b2f1c9af8" type="" style="Table" enable="true">
	<name><![CDATA[sql_active]]></name>
	<description><![CDATA[]]></description>
	<tooltip><![CDATA[]]></tooltip>
	<drillclass><![CDATA[]]></drillclass>
	<CustomValues>
		<TYPE>horizontal</TYPE>
	</CustomValues>
	<query>
		<sql><![CDATA[SELECT * FROM
       (SELECT status,inst_id,sid,SESSION_SERIAL# as Serial,username,sql_id,SQL_PLAN_HASH_VALUE,
        TO_CHAR(sql_exec_start,'dd-mon-yyyy hh24:mi:ss') AS sql_exec_start,
         ROUND(elapsed_time/1000000/60)                      AS "Elapsed (min)",MODULE,program,
         ROUND(cpu_time    /1000000)                      AS "CPU (s)",
         substr(sql_text,1,50) sql_text
       FROM gv$sql_monitor where status='EXECUTING' and module not like '%emagent%' 
       ORDER BY sql_exec_start,status
       )]]></sql>
	</query>
