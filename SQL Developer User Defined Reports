<display id="a10a4003-0193-1000-8001-ac1b2f1c2380" type="" style="Table" enable="true">
	<name><![CDATA[top_10_objects]]></name>
	<description><![CDATA[]]></description>
	<tooltip><![CDATA[]]></tooltip>
	<drillclass><![CDATA[]]></drillclass>
	<CustomValues>
		<TYPE>horizontal</TYPE>
	</CustomValues>
	<query>
		<sql><![CDATA[SELECT *
FROM
  (SELECT s.owner,
    s.segment_name,
    s.segment_type,
    s.tablespace_name,
    l.table_name ,
    bytes/1024/1024/1024 Size_GB
  FROM dba_segments s
  LEFT OUTER JOIN dba_lobs l
  ON s.segment_name=l.segment_name
  ORDER BY 6 DESC
  )
WHERE rownum<10]]></sql>
	</query>
