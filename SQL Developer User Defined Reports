	<name><![CDATA[top_sessions_by_cpu_time]]></name>
	<description><![CDATA[]]></description>
	<tooltip><![CDATA[]]></tooltip>
	<drillclass><![CDATA[]]></drillclass>
	<CustomValues>
		<TYPE>horizontal</TYPE>
	</CustomValues>
	<query>
		<sql><![CDATA[SELECT  s.sid, s.serial#, s.username, s.status,  p.spid AS os_process_id,  ss.value AS cpu_time,
    sq.sql_id, SUBSTR(sq.sql_text, 1, 100) AS sql_text
FROM
    v$session s
    JOIN v$process p ON s.paddr = p.addr
    JOIN v$sesstat ss ON s.sid = ss.sid
    JOIN v$statname st ON ss.statistic# = st.statistic#
    LEFT JOIN v$sql sq ON s.sql_id = sq.sql_id
WHERE
    st.name = 'CPU used by this session'
    AND s.status = 'ACTIVE'  -- Only active sessions
ORDER BY
    ss.value DESC]]></sql>
	</query>
		<pdf version="VERSION_1_7" compression="CONTENT">
