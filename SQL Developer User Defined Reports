<display id="a110f857-0193-1000-8002-ac1b2f1c2380" type="" style="Table" enable="true">
	<name><![CDATA[table_size]]></name>
	<description><![CDATA[]]></description>
	<tooltip><![CDATA[]]></tooltip>
	<drillclass><![CDATA[]]></drillclass>
	<CustomValues>
		<TYPE>horizontal</TYPE>
	</CustomValues>
	<query>
		<sql><![CDATA[select segment_name,segment_type, sum(bytes/1024/1024/1024) GB,owner
 from dba_segments
 where segment_name=:table_name
group by segment_name,segment_type,owner]]></sql>
