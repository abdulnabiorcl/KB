	<query>
		<sql><![CDATA[SELECT LO.SESSION_ID,
         LO.ORACLE_USERNAME,
         LO.OS_USER_NAME,
         LO.PROCESS,
         DO.OBJECT_NAME,
         DO.OWNER,
         DECODE (LO.LOCKED_MODE,
                 0, 'NONE',
                 1, 'NULL',
                 2, 'ROW SHARE (SS)',
                 3, 'ROW EXCL (SX)',
                 4, 'SHARE',
                 5, 'SHARE ROW EXCL (SSX)',
                 6, 'EXCLUSIVE',
                 TO_CHAR (LO.LOCKED_MODE))
             MODE_HELD
    FROM GV$LOCKED_OBJECT LO, DBA_OBJECTS DO
   WHERE LO.OBJECT_ID = DO.OBJECT_ID
ORDER BY 5]]></sql>
	</query>
