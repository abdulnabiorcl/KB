<display id="a14ca6c5-0193-1000-8005-ac1b2f1c2380" type="" style="Table" enable="true">
	<name><![CDATA[object_size_by_owner]]></name>
	<description><![CDATA[		]]></description>
	<tooltip><![CDATA[]]></tooltip>
	<drillclass><![CDATA[]]></drillclass>
	<CustomValues>
		<TYPE>horizontal</TYPE>
	</CustomValues>
	<query>
		<sql><![CDATA[select owner, round(sum(bytes)/1024/1024,2) Size_MB,round(sum(bytes)/1024/1024/1024,2) Size_GB,round(sum(bytes)/1024/1024/1024/1024,2) Size_TB from dba_segments
group  by owner order by 2 desc]]></sql>
	</query>
