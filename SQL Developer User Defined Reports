<display id="bb8c15eb-0193-1000-8001-ac1b2f1c9af8" type="" style="Table" enable="true">
	<name><![CDATA[long_running_sessions_kill]]></name>
	<description><![CDATA[]]></description>
	<tooltip><![CDATA[]]></tooltip>
	<drillclass><![CDATA[]]></drillclass>
	<CustomValues>
		<TYPE>horizontal</TYPE>
	</CustomValues>
	<query>
		<sql><![CDATA[SELECT    'ALTER SYSTEM KILL SESSION '''|| b.SID|| ','|| b.SERIAL#|| ',@'|| b.INST_ID|| ''' IMMEDIATE;'
           LONG_RUNNING_KILL_COMMAND
           from gv$session b,gv$process a,gv$sqlcommand c
 where b.paddr = a.addr 
 and a.inst_id = b.inst_id  
 and type='USER' and b.username not in('SYSRAC','DBSNMP')
 and c.command_type=b.command
 and last_call_et/60> :MIN
 order by logon_time]]></sql>
		<binds>
