		<query>
			<sql><![CDATA[SELECT tablespace_name,file_name,autoextensible "Auto Extend",round(bytes/1024/1024,2) "Used in MB",round(maxbytes/1024/1024,2) "Max in MB",round((maxbytes/1024/1024-bytes/1024/1024),2) "Free in MB",increment_by
FROM dba_data_files where tablespace_name=:TABLESPACE_NAME order by file_name]]></sql>
			<binds>
