			<sql><![CDATA[select a.target_name as DISPLAY_NAME ,g.property_value DB_NAME,f.property_value as DB_Version, a.property_value as CLUSTERNAME, b.property_value as TENANT, c.property_value as ENV, e.property_value OPEN_MODE,d.DB_UNIQUE_NAME as STBY_DB_NAME
from 
sysman.MGMT$TARGET a1,
sysman.MGMT$TARGET_PROPERTIES a,
sysman.MGMT$TARGET_PROPERTIES b,
sysman.MGMT$TARGET_PROPERTIES c,
sysman.MGMT$TARGET_PROPERTIES e,
sysman.CM$MGMT_HA_DG_TARGET_SUMMARY_E d,
sysman.MGMT$TARGET_PROPERTIES f,
sysman.MGMT$TARGET_PROPERTIES g
where a.target_type='rac_database'
and a1.target_type='rac_database'
and a.property_name='ClusterName'
and b.property_name='orcl_gtp_location'
and c.property_name='orcl_gtp_lifecycle_status'
and e.property_name='OpenMode'
and f.property_name='DBVersion'
and g.property_name='DBName'
and a1.target_guid=a.target_guid (+)
and a1.target_guid=b.target_guid (+)
and a1.target_guid=c.target_guid (+)
and a1.target_guid=e.target_guid (+)
and a1.target_guid=f.target_guid (+)
and a1.target_guid=g.target_guid (+)
and e.property_value='READ WRITE'
and a.target_name=d.CM_TARGET_NAME (+)
and a.target_name=d.PRMY_DB_UNIQUE_NAME (+)
order by 1]]></sql>
