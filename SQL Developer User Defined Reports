<display id="fddf0ca5-0193-1000-800c-ac1b2f1c75c3" type="" style="Table" enable="true">
	<name><![CDATA[blocking_session_blocking_sql_text]]></name>
	<description><![CDATA[]]></description>
	<tooltip><![CDATA[]]></tooltip>
	<drillclass><![CDATA[]]></drillclass>
	<CustomValues>
		<TYPE>horizontal</TYPE>
	</CustomValues>
	<query>
		<sql><![CDATA[SELECT *
    FROM GV$OPEN_CURSOR OC
   WHERE     OC.INST_ID = :TYPE_BLOCKING_INSTANCE_ID
         AND OC.SID = :TYPE_BLOCKING_SID
         AND (   OC.SQL_TEXT LIKE 'INSERT%'
              OR OC.SQL_TEXT LIKE 'UPDATE%'
              OR OC.SQL_TEXT LIKE 'DELETE%')
         AND OC.CURSOR_TYPE = 'OPEN'
ORDER BY OC.LAST_SQL_ACTIVE_TIME]]></sql>
